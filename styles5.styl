body
  font-family: Arial, sans-serif
  margin: 0
  padding: 0
  box-sizing: border-box

.center
  display: grid
  grid-template-rows: auto auto auto
  grid-template-columns: 1fr
  gap: 20px
  padding: 20px

.logo-section
  display: flex
  align-items: center
  justify-content: center
  gap: 20px

.logo
  width: 100px
  height: 100px
  background-image: url('data:image/png;base64,')
  background-size: contain
  background-repeat: no-repeat

.slogan
  font-size: 1.5rem

.content
  display: flex
  justify-content: center
  gap: 20px

.content-box
  display: flex
  align-items: center
  gap: 20px
  max-width: 600px
  width: 100%

.image-placeholder1, .image-placeholder2
  width: 150px
  height: 150px
  background-size: cover
  background-position: center

.image-placeholder1
  background-image: url('https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRHojDXK1VXUp7V-zRqpfkVsfxJ0t-uO4a5oQ&s')

.image-placeholder2
  background-image: url('https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTQ7bTZaJoT1Y_aaqCoXnGIBOVirQI8SMph-w&s')

.text-content
  display: flex
  flex-direction: column
  justify-content: center

.text-content h3
  margin: 0

.subscribe-section
  text-align: center

.input-section
  margin-top: 10px

.input-section input
  padding: 10px
  font-size: 1rem
  width: 200px

.input-section button
  padding: 10px
  font-size: 1rem
  cursor: pointer

a:link
  text-decoration: none

footer
  display: flex
  justify-content: space-between
  padding: 20px
  background: #f8f8f8
  border-top: 1px solid #ccc

footer
  font-size: 20px
  font-weight: bold

footer .list
  display: flex
  gap: 30px

footer .list-item
  font-size: 14px



